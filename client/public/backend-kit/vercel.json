{
  "version": 2,
  "buildCommand": "npm install",
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "false"
  },
  "functions": {
    "api/maps.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
